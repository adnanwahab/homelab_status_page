
{
  "manifest_version": 3,
  "name": "WebSocket Logger",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["helper_ws_client_chrome_extension.js"]
    }
  ],
  "permissions": [

    "webRequest",
    "webRequestBlocking",
    "http://localhost/*",
    "ws://localhost/*"


]
}